{
    "extends": "../tsconfig.base.json",
    "compilerOptions": {
        "outDir": "./dist/",
        "baseUrl": ".",
        "paths": {
            "*": ["src/*"],
            "@app/*": ["src/app/*"],
            "@pages/*": ["src/pages/*"],
            "@widgets/*": ["src/widgets/*"],
            "@features/*": ["src/features/*"],
            "@entities/*": ["src/entities/*"],
            "@shared/*": ["src/shared/*"],
            "@assets/*": ["src/shared/assets/*"],
            "@icons/*": ["src/shared/assets/icons/*"]
        },
    }
}